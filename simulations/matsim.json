{
  "latest": "0.3",
  "stable": "0.3",
  "0.1": {
    "command": "~/sim-city-client/simulations/matsim.py",
    "parameters": [
      {
        "name": "model",
        "type": "choice",
        "choices": ["tutorial", "blr"],
        "default": "tutorial",
        "dtype": "str",
        "description": "model to run, specified in $MATSIM/config/$MODEL/config.xml"
      }
    ]
  },
  "0.2": {
    "command": "~/bangalore-matsim-0.2/scripts/matsim.py",
    "parameters": [
      {
        "title": "commute percentage",
        "name": "commute-percentage",
        "type": "number",
        "min": "0",
        "max": "100",
        "default": "5",
        "dtype": "float",
        "description": "percentage of the population (totalling 8.5 million) that commutes"
      }
    ]
  },
  "0.3": {
    "command": "~/bangalore-matsim-0.3/matsim.py",
    "parameters": [
      {
        "title": "scenario name",
        "name": "name",
        "type": "str",
        "min_length": "3"
      },
      {
        "title": "commute percentage",
        "name": "commute-percentage",
        "type": "number",
        "min": "0",
        "max": "100",
        "default": "5",
        "dtype": "float",
        "description": "percentage of the population (totalling 8.5 million) that commutes"
      }
    ]
  },
  "0.4": {
    "command": "~/bangalore-matsim-0.4/matsim.py",
    "parameters": [
      {
        "title": "ensemble name",
        "name": "ensemble",
        "type": "str",
        "min_length": "1"
      },
      {
        "title": "scenario name",
        "name": "simulation",
        "type": "str",
        "min_length": "1"
      },
      {
        "title": "commute percentage",
        "name": "populationSampleFactor",
        "type": "number",
        "dtype": "float",
        "min": "0",
        "max": "100",
        "default": "1",
        "description": "percentage of the population (totalling 8.5 million) that commutes"
      },
      {
        "title": "Fire stations",
        "name": "fireStations",
        "type": "list",
        "min_length": 1,
        "default": "blr_firestations",
        "contents": {
          "type": "point2d",
          "properties": [
            {"name": "name", "type": "str"},
          ]
        }
      },
      {
        "title": "Fire",
        "name": "fires",
        "type": "list",
        "min_length": 1,
        "max_length": 1,
        "default": "blr_fires",
        "contents": {
          "type": "point2d"
        }
      }
    ]
  }
}
